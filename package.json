{
  "name": "vite-plugin-tauri",
  "version": "1.0.0",
  "description": "Integrate Tauri in a Vite project to build cross-platform apps.",
  "repository": "https://github.com/amrbashir/vite-plugin-tauri.git",
  "author": "Amr Bashir <amr.bashir2015@gmail.com>",
  "license": "MIT",
  "files": [
    "bin"
  ],
  "bin": {
    "vite-tauri": "bin/vite-tauri.js"
  },
  "type": "module",
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "tsup src/vite-tauri.ts -d bin --format esm --clean --env.NODE_ENV production",
    "dev": "tsup src/vite-tauri.ts --watch -d bin --format esm --clean",
    "type:check": "tsc --noEmit",
    "format": "prettier --write src --ignore-path .gitignore",
    "format:check": "prettier --check \"./**/*.{ts,json}\" --ignore-path .gitignore",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "enquirer": "2.3.6",
    "fast-glob": "3.2.11",
    "kolorist": "1.5.1"
  },
  "peerDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.0",
    "vite": "^2.0.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.0",
    "@types/node": "17.0.13",
    "conventional-changelog-cli": "2.2.2",
    "prettier": "2.5.1",
    "taze": "0.4.6",
    "tsup": "5.11.13",
    "typescript": "4.5.5",
    "vite": "2.7.13"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "vite-plugin",
    "vite",
    "tauri"
  ]
}
