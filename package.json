{
  "name": "vite-plugin-tauri",
  "version": "2.3.0",
  "description": "Integrate Tauri in a Vite project to build cross-platform apps.",
  "repository": "https://github.com/amrbashir/vite-plugin-tauri.git",
  "author": "Amr Bashir <amr.bashir2015@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "watch": "tsup src/index.ts --watch --format esm --dts --clean",
    "ts:check": "tsc --noEmit",
    "format": "prettier --write \"./**/*.{js,ts,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"./**/*.{js,ts,json}\" --ignore-path .gitignore",
    "deps:up": "pnpm dlx taze latest -w -i"
  },
  "dependencies": {
    "fast-glob": "3.2.12",
    "kolorist": "1.6.0"
  },
  "peerDependencies": {
    "@tauri-apps/cli": ">= 1",
    "vite": ">= 2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.1.1",
    "@types/node": "18.7.23",
    "prettier": "2.7.1",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "vite": "3.1.4"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "tauri",
    "tauri-plugin"
  ]
}
